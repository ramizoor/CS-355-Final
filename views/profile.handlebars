<h1>{{user.name}}'s Profile</h1>

<h3>Your Recipes</h3>
<ul>
  {{#each user.recipes}}
    <li><a href="/recipes/{{id}}">{{title}}</a></li>
  {{/each}}
</ul>

<h3>Favorites</h3>
<ul>
  {{#each user.favorites}}
    <li><a href="/recipes/{{recipe.id}}">{{recipe.title}}</a></li>
  {{/each}}
</ul>

