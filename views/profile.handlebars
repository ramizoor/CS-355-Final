<h1>{{user.name}}'s Profile</h1>

<!-- List user's own recipes -->
<h3>Your Recipes</h3>
<ul>
  {{#each user.recipes}}
    <li><a href="/recipes/{{id}}">{{title}}</a></li>
  {{/each}}
</ul>

<!-- List user's favorited recipes -->
<h3>Favorites</h3>
<ul>
  {{#each user.favorites}}
    <li><a href="/recipes/{{recipe.id}}">{{recipe.title}}</a></li>
  {{/each}}
</ul>

<!-- Back to homepage -->
<a href="/">‚Üê Back to Home</a>
